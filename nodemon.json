{
    "verbose": false,
    "debug": false,
    "exec": "ts-node  ./bin/start.ts",
    "ignore": [
      "mochawesome-report",
      "node_modules",
      "./test",
      "**/*.d.ts",
      "*.test.ts",
      "*.spec.ts",
      "fixtures/*",
      "test/**/*",
      "docs/*"
    ],
    "events": {
      "restart": ""
    },
    "watch": ["./**"],
    "ext": "js json ts tsx",
    "legacy-watch": false
}